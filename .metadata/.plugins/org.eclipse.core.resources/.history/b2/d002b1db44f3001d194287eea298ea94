$(function() {
	 const PATH = getContextPath();
	 $("#insert-btn").click(function() {
		const arr = $("#insert-form").serializeArray();
		const obj = {};
		$.each(arr, (index, item)=>{
			obj[item.name] = item.value;
		});
		$.ajax({
			url : PATH + "/equi/equipmentInsert.do",
			method:"POST",
			data: obj,
			beforeSend : () => {
				$("#my-spinner").show();
			},
			success:(res)=>{
				$("#my-spinner").hide();
				if (res === '1') {
					
				}else{
					alert("추가 실패");
				}
				location.href = "${path}/emp/empList.do";
			},
			error:(err)=>{
				console.error(err);
			}
		});
	});
	});